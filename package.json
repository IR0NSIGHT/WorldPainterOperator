{
  "name": "worldpainteroperator", 
  "version": "1.0.0", 
  "description": "advanced operations from config file for WorldPainter", 
  "main": "index.js", 
  "scripts": {
    "compile": "tsc -p ./tsconfig.json", 
    "copyConfigs": "mkdir ./dist/AdvancedOperator/configs & cp -r ./src/configs ./dist/AdvancedOperator/configs", 
    "copyReadme": "cp readme.md ./dist/AdvancedOperator/readme.md", 
    "bundle": "webpack --mode production", 
    "build": "npm run compile && npm run bundle && npm run insertHeader", 
    "prepareDeploy": "npm run copyConfigs & npm run copyReadme & npm run zip", 
    "zip": "cd ./dist/AdvancedOperator & rm ../AdvancedOperator.zip & zip -r ../AdvancedOperator.zip .", 
    "insertHeader": "bash ./shellscripts/insertHeader.sh", 
    "lint": "eslint ./src --fix --config .eslintrc.json", 
    "test": "jest ./src --verbose"
  }, 
  "repository": {
    "type": "git"                                                      , 
    "url" : "git+https://github.com/IR0NSIGHT/WorldPainterOperator.git"  
  }, 
  "keywords": ["WorldPainter", "Operator", "minecraft"], 
  "author": "IR0NSIGHT", 
  "license": "MIT", 
  "bugs": {"url": "https://github.com/IR0NSIGHT/WorldPainterOperator/issues"}, 
  "homepage": "https://github.com/IR0NSIGHT/WorldPainterOperator#readme", 
  "devDependencies": {
    "@types/jest"                           : "^29.5.1" , 
    "@types/node"                           : "^18.16.0", 
    "@typescript-eslint/eslint-plugin"      : "^5.59.1" , 
    "esbuild-loader"                        : "2.15.1"  , 
    "eslint"                                : "^8.39.0" , 
    "eslint-config-standard-with-typescript": "^34.0.1" , 
    "eslint-plugin-import"                  : "^2.27.5" , 
    "eslint-plugin-n"                       : "^15.7.0" , 
    "eslint-plugin-promise"                 : "^6.1.1"  , 
    "jest"                                  : "^29.5.0" , 
    "ts-jest"                               : "^29.1.0" , 
    "ts-node"                               : "^10.9.1" , 
    "typescript"                            : "^5.0.4"  , 
    "webpack"                               : "^5.80.0" , 
    "webpack-cli"                           : "^5.0.2"    
  }
}
